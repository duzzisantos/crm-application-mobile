<LandingPage>
    size_hint: 1, 1
    pos_hint: {'center_x': .5, 'center_y': .5}

    do_default_tab: False

    tab_pos: 'top_left'

    TabbedPanelItem:
        text: "Managers"
        BoxLayout:
        Header:
            text: "Managers List"
        AccountManagers: AccountManagers().run()
            
        

    TabbedPanelItem:
        text: "Campaigns"
        BoxLayout:
        Header:
            text: "Campaigns List"
        Campaigns: Campaigns().run()
       

    TabbedPanelItem:
        text: "Customers"
        BoxLayout:
        Header:
            text: "Customers List"
        Customers: Customer().run()
        

    TabbedPanelItem:
        text: "Enrollments"
        BoxLayout:
        Label:
            text: "Enrollments List"
        Enrollments().run()
        


    TabbedPanelItem:
        text: "Products"
        BoxLayout:
        Header:
            text: "Products List"
        Products().run()
        